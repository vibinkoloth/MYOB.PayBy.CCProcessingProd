    <Sql TableName="FixCustomerPaymentMethodExpireDate" CustomScript="#CDATA">
        <CDATA name="CustomScript"><![CDATA[
GO
--[mssql: Skip]
UPDATE CustomerPaymentMethod
        INNER JOIN
    CCProcessingCenter ON CustomerPaymentMethod.CCProcessingCenterID = CCProcessingCenter.ProcessingCenterID 
SET 
    ExpirationDate = ADDDATE(ExpirationDate,
        INTERVAL 1 MONTH)
WHERE
    ProcessingTypeName = 'MYOB.PayBy.CCProcessing.V2.PayByProcessingPluginV2';

GO]]></CDATA>
    </Sql>
    <Sql TableName="SkipCCExpireDateCheck" CustomScript="#CDATA">
        <CDATA name="CustomScript"><![CDATA[update CustomerPaymentMethod set MAKLExpirationDate = ExpirationDate;
update CustomerPaymentMethod set ExpirationDate = null;]]></CDATA>
    </Sql>
